    "keywords": [
        "dick",
        "laravel",
        "tabacitu",
        "cristian tabacitu",
        "updivision",
        "dick admin",
        "menu manager",
        "menu item manager",
        "laravel menu",
        "dick menu"
    ],
    "homepage": "https://github.com/tabacitu/menumanager",
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Tabacitu",
            "email": "hello@tabacitu.ro",
